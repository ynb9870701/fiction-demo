/**app.wxss**/
page{
    background-color: #f3f3f3;
}
.container{
  display: flex;
  flex-direction: column;
  width: 100%;
  min-height: 100%;
}
/* margin */
.m{
  margin: 20rpx;
}

.m-t-sm{
  margin-top: 10rpx;
}
.m-t{
  margin-top: 20rpx;
}
.m-t-xs{
  margin-top: 30rpx;
}
.m-t-md{
  margin-top: 40rpx;
}

.m-b-sm{
  margin-bottom: 10rpx;
}
.m-b{
  margin-bottom: 20rpx;
}
.m-b-xs{
  margin-bottom: 30rpx;
}
.m-b-md{
  margin-bottom: 40rpx;
}

.m-r-sm{
  margin-right: 10rpx;
}
.m-r{
  margin-right: 20rpx;
}
.m-r-xs{
  margin-right: 30rpx;
}
.m-r-md{
  margin-right: 40rpx;
}

.m-l-sm{
  margin-left: 10rpx;
}
.m-l{
  margin-left: 20rpx;
}
.m-l-xs{
  margin-left: 30rpx;
}
.m-l-md{
  margin-left: 40rpx;
}
/* padding */
.p-sm{
  padding: 10rpx;
}
.p{
  padding: 20rpx;
}
.p-xs{
  padding: 30rpx;
}
.p-md{
  padding: 40rpx;
}
.p-t-sm{
  padding-top: 10rpx;
}
.p-t{
  padding-top: 20rpx;
}
.p-t-xs{
  padding-top: 30rpx;
}
.p-t-md{
  padding-top: 40rpx;
}

.p-b-sm{
  padding-bottom: 10rpx;
}
.p-b{
  padding-bottom: 20rpx;
}
.p-b-xs{
  padding-bottom: 30rpx;
}
.p-b-md{
  padding-bottom: 40rpx;
}

.p-r-sm{
  padding-right: 10rpx;
}
.p-r{
  padding-right: 20rpx;
}
.p-r-xs{
  padding-right: 30rpx;
}
.p-r-md{
  padding-right: 40rpx;
}

.p-l-sm{
  padding-left: 10rpx;
}
.p-l{
  padding-left: 20rpx;
}
.p-l-xs{
  padding-left: 30rpx;
}
.p-l-md{
  padding-left: 40rpx;
}
/* 背景色 */
.bg-purple{
  background-color: #7266ba;
}
.bg-white{
  background-color: #fff;
}
.bg-info{
  background-color: #23b7e5;
}
.bg-light{
  background-color: #e4eaec;
}
.bg-f3{
  background-color: #f3f3f3;
}
/* 字体颜色 */
.text-info{
  color: #23b7e5;
}
.text-default{
  color: #fff;
}
.text-primary{
  color: #9ea8af;
}
.text-purple{
  color: #7266ba;
}
.text-warning{
  color: #f9d744;
}
.text-danger{
  color: #f21b1b;
}
.text-muted {
  color: #999;
}
.text-black{
  color: #000;
}
/* 字体大小 */
.text-bold{
  font-weight: bold;
}
.text-xxl{
  font-size: 72rpx;
}
.text-xl{
  font-size: 48rpx;
}
.text-lg {
  font-size: 36rpx;
}
.text-lg-i {
  font-size: 36rpx !important;
}
.text-md {
  font-size: 32rpx;
}
.text-base {
  font-size: 28rpx;
}
.text-sm {
  font-size: 26rpx;
}
.text-xs {
  font-size: 24rpx;
}
.text-left{
  text-align: left;
}
.text-center{
  text-align: center;
}
.text-right{
  text-align: right;
}
/* 行内转换 */
.block{
  display: block;
}
.inline{
  display: inline;
}
.inline-block {
  display: inline-block;
}
/* flex布局 */
.flex-wrap {
  display: flex;
}
.flex-y{
  flex-direction: column;
}
.flex-lr{
  justify-content: space-between;
}
.flex-center-x{
  display: flex;
  justify-content: center;
}
.flex-center-y {
  display: flex;
  align-items: center;
}
.flex-1{
  flex: 1;
}
.flex-2{
  flex: 2;
}
.flex-3{
  flex: 3;
}
/*  边框border  */
.b {
  border: 1px solid rgba(0, 0, 0, 0.05);
}
.b-a {
  border: 1px solid #dee5e7;
}
.b-t {
  border-top: 1px solid #dee5e7;
}
.b-r {
  border-right: 1px solid #dee5e7;
}
.b-b {
  border-bottom: 1px solid #dee5e7;
}
.b-l {
  border-left: 1px solid #dee5e7;
}


.lh-44{
  line-height: 44rpx;
}


.text-2{
  overflow:hidden;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
  word-break:break-all;
}






















